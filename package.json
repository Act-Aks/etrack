{
    "name": "etrack",
    "main": "index.ts",
    "version": "1.0.0",
    "scripts": {
        "start": "expo start",
        "reset-project": "node ./scripts/reset-project.js",
        "web": "expo start --web",
        "test": "jest --watchAll",
        "lint": "expo lint",
        "prettier": "prettier --write .",
        "update:all": "yarn upgrade-interactive",
        "update": "yarn expo install expo@latest && yarn expo install --fix",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "cmt": "npx git-cz",
        "build:dev:android": "yarn eas build --profile development --platform android",
        "build:dev:ios": "yarn eas build --profile development --platform ios",
        "build:dev-sim:ios": "yarn eas build --profile development-simulator --platform ios",
        "build:prod:android": "yarn eas build --profile production --platform android",
        "build:prod:ios": "yarn eas build --profile production --platform ios",
        "release:android": "bundle exec fastlane android release",
        "preinstall": "npx only-allow yarn",
        "upgrade-self": "yarn set version latest && yarn",
        "up:i": "yarn upgrade-interactive",
        "prepare": "husky",
        "cq": "yarn prettier && yarn lint"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "@backpackapp-io/react-native-toast": "^0.13.0",
        "@expo/vector-icons": "^14.0.4",
        "@react-native-async-storage/async-storage": "2.1.2",
        "@react-navigation/bottom-tabs": "^7.2.1",
        "@react-navigation/native": "^7.0.15",
        "@tanstack/react-query": "^5.67.3",
        "@tanstack/react-query-devtools": "^5.67.3",
        "axios": "^1.8.3",
        "expo": "^52.0.38",
        "expo-blur": "^14.0.3",
        "expo-constants": "^17.0.8",
        "expo-dev-client": "^5.0.13",
        "expo-font": "^13.0.4",
        "expo-haptics": "^14.0.1",
        "expo-image": "^2.0.6",
        "expo-image-picker": "^16.0.6",
        "expo-linking": "^7.0.5",
        "expo-router": "^4.0.18",
        "expo-splash-screen": "^0.29.22",
        "expo-status-bar": "^2.0.1",
        "expo-symbols": "^0.2.2",
        "expo-system-ui": "^4.0.8",
        "expo-web-browser": "^14.0.2",
        "firebase": "^11.4.0",
        "phosphor-react-native": "^2.3.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.7",
        "react-native-edge-to-edge": "^1.4.3",
        "react-native-gesture-handler": "^2.24.0",
        "react-native-nitro-modules": "^0.25.1",
        "react-native-reanimated": "^3.17.1",
        "react-native-safe-area-context": "5.3.0",
        "react-native-screens": "^4.9.1",
        "react-native-unistyles": "3.0.0-nightly-20250226",
        "react-native-web": "^0.19.13",
        "react-native-webview": "13.13.4"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@react-native/normalize-colors": "^0.78.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/exec": "^7.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/release-notes-generator": "^14.0.3",
        "@tanstack/eslint-plugin-query": "^5.67.2",
        "@types/jest": "^29.5.14",
        "@types/react": "^18.3.12",
        "@types/react-test-renderer": "^19.0.0",
        "eas-cli": "^15.0.15",
        "eslint": "^9.0.0",
        "eslint-config-expo": "~8.0.1",
        "jest": "^29.7.0",
        "jest-expo": "^52.0.6",
        "prettier": "^3.5.3",
        "react-native-svg": "^15.11.2",
        "react-test-renderer": "19.0.0",
        "semantic-release": "^24.2.3",
        "semantic-release-react-native": "^1.12.2",
        "typescript": "^5.8.2"
    },
    "engines": {
        "node": ">20",
        "yarn": ">=4.6.0"
    },
    "private": true,
    "packageManager": "yarn@4.7.0"
}
